{
    "name": "bierpongregeln",
    "description": "This is a collection of rulesets for beer pong that is built using Docusaurus 2 and served with GitHub Pages.",
    "version": "2.10.1",
    "private": true,
    "scripts": {
        "docusaurus": "docusaurus",
        "start": "docusaurus start",
        "build": "docusaurus build",
        "swizzle": "docusaurus swizzle",
        "deploy": "docusaurus deploy",
        "clear": "docusaurus clear",
        "serve": "docusaurus serve",
        "write-translations": "python3 scripts/update-footer-en.py",
        "write-heading-ids": "docusaurus write-heading-ids",
        "validate-json": "ajv validate -s tournaments.schema.json tournaments.json",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "typecheck": "tsc",
        "update-version": "python3 scripts/updateversion.py"
    },
    "dependencies": {
        "@docusaurus/preset-classic": "^3.7.0",
        "@mdx-js/react": "^3.1.0",
        "clsx": "^2.1.1",
        "globals": "^16.0.0",
        "prism-react-renderer": "^2.4.1",
        "react": "^19.1.0",
        "react-dom": "^19.0.0"
    },
    "devDependencies": {
        "@docusaurus/tsconfig": "^3.7.0",
        "@iconify/react": "^5.2.1",
        "@jirutka/ajv-cli": "^6.0.0",
        "@typescript-eslint/eslint-plugin": "^8.28.0",
        "eslint": "^9.23.0",
        "typescript": "~5.7.3"
    },
    "browserslist": {
        "production": [
            ">0.5%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 3 chrome version",
            "last 3 firefox version",
            "last 5 safari version"
        ]
    },
    "engines": {
        "node": ">=18.0"
    }
}